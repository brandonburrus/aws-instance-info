{
  "name": "ec2-details",
  "description": "Meta-information on AWS EC2 instances such as size, memory, and more",
  "version": "0.1.0",
  "author": "Brandon Burrus <brandon@burrus.io>",
  "license": "MIT",
  "keywords": [
    "aws",
    "ec2",
    "instance",
    "metadata",
    "details",
    "information",
    "meta-information"
  ],
  "homepage": "https://github.com/brandonburrus/ec2-details",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonburrus/ec2-details.git"
  },
  "bugs": {
    "url": "https://github.com/brandonburrus/ec2-details/issues"
  },
  "files": ["dist", "data", "README.md", "LICENSE"],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./data/ec2/*": "./data/ec2/*",
    "./data/rds/*": "./data/rds/*",
    "./data/elasticache/*": "./data/elasticache/*"
  },
  "scripts": {
    "build": "tsc",
    "generate": "python scripts/generate.py",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "format": "biome format",
    "format:fix": "biome format --write"
  },
  "lint-staged": {
    "*.ts": [
      "biome check --write --staged",
      "vitest related --passWithNoTests --run"
    ],
    "*.json": ["biome check --write --staged"]
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@vitest/coverage-v8": "^4.0.13",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.14",
    "typescript": "^5.8.3",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "lru-cache": "^11.2.2"
  }
}
