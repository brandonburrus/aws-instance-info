{
  "name": "ec2-details",
  "description": "Meta-information on AWS EC2 instances such as size, memory, and more",
  "version": "0.0.0",
  "author": "Brandon Burrus <brandon@burrus.io>",
  "license": "MIT",
  "keywords": [
    "aws",
    "ec2",
    "instance",
    "metadata",
    "details",
    "information",
    "meta-information"
  ],
  "homepage": "https://github.com/brandonburrus/ec2-details",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonburrus/ec2-details.git"
  },
  "bugs": {
    "url": "https://github.com/brandonburrus/ec2-details/issues"
  },
  "packageManager": "pnpm@10.12.1",
  "files": ["dist", "README.md", "LICENSE"],
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepare": "husky",
    "format": "biome format",
    "format:fix": "biome format --write"
  },
  "lint-staged": {
    "*.ts": ["biome check --write --staged", "jest --passWithNoTests"],
    "*.json": ["biome check --write --staged"]
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "^29.5.14",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
